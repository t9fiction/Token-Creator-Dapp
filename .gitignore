# Uncomment the line below to track it
# .env.local

# Ignore compiled and cache files
/frontend/.cache/
frontend/dist/
frontend/build/
frontend/.turbo/
coverage

# production build
/build/

# Miscellaneous
.DS_Store
*.pem

# TypeScript build information
*.tsbuildinfo
next-env.d.ts

# Vercel deployment files
.vercel
